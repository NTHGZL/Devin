{
  "kind": "collectionType",
  "collectionName": "sessions",
  "info": {
    "name": "Session",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "wine": {
      "model": "wine",
      "via": "sessions"
    },
    "stores": {
      "collection": "store",
      "via": "sessions"
    },
    "Nose": {
      "type": "dynamiczone",
      "components": [
        "session.question",
        "session.advice"
      ]
    },
    "Eyes": {
      "type": "dynamiczone",
      "components": [
        "session.question",
        "session.advice"
      ]
    },
    "Taste": {
      "type": "dynamiczone",
      "components": [
        "session.question",
        "session.advice"
      ]
    }
  }
}
